<?xml version="1.0" encoding="UTF-8"?>
<ns4:map xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ns3="http://www.w3.org/2001/XMLSchema" xmlns:ns4="http://www.w3.org/2015/EXI/json">
    <ns4:_47.>
        <ns4:map>
            <ns4:storage>
                <ns4:map>
                    <ns4:type>
                        <ns4:string>disk</ns4:string>
                    </ns4:type>
                    <ns4:device>
                        <ns4:string>/dev/sda1</ns4:string>
                    </ns4:device>
                </ns4:map>
            </ns4:storage>
            <ns4:fstype>
                <ns4:string>btrfs</ns4:string>
            </ns4:fstype>
            <ns4:readonly>
                <ns4:boolean>true</ns4:boolean>
            </ns4:readonly>
        </ns4:map>
    </ns4:_47.>
    <ns4:_47.var>
        <ns4:map>
            <ns4:storage>
                <ns4:map>
                    <ns4:type>
                        <ns4:string>disk</ns4:string>
                    </ns4:type>
                    <ns4:label>
                        <ns4:string>8f3ba6f4-5c70-46ec-83af-0d5434953e5f</ns4:string>
                    </ns4:label>
                </ns4:map>
            </ns4:storage>
            <ns4:fstype>
                <ns4:string>ext4</ns4:string>
            </ns4:fstype>
            <ns4:options>
                <ns4:array>
                    <ns4:string>nosuid</ns4:string>
                </ns4:array>
            </ns4:options>
        </ns4:map>
    </ns4:_47.var>
    <ns4:_47.tmp>
        <ns4:map>
            <ns4:storage>
                <ns4:map>
                    <ns4:type>
                        <ns4:string>tmpfs</ns4:string>
                    </ns4:type>
                    <ns4:sizeInMB>
                        <ns4:number>64E0</ns4:number>
                    </ns4:sizeInMB>
                </ns4:map>
            </ns4:storage>
        </ns4:map>
    </ns4:_47.tmp>
    <ns4:_47.var_47.www>
        <ns4:map>
            <ns4:storage>
                <ns4:map>
                    <ns4:type>
                        <ns4:string>nfs</ns4:string>
                    </ns4:type>
                    <ns4:server>
                        <ns4:string>my.nfs.server</ns4:string>
                    </ns4:server>
                    <ns4:remotePath>
                        <ns4:string>/exports/mypath</ns4:string>
                    </ns4:remotePath>
                </ns4:map>
            </ns4:storage>
        </ns4:map>
    </ns4:_47.var_47.www>
</ns4:map>
